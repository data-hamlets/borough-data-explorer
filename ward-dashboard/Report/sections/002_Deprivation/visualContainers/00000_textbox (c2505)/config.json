                  {
                    "value": "A household is deprived in a dimension if they meet one or more of the following conditions:",
                    "textStyle": {
                      "fontSize": "14pt"
                    }
                  }
                ]
              },
              {
                "textRuns": [
                  {
                    "value": "employment",
                    "textStyle": {
                      "fontWeight": "bold",
                      "fontSize": "14pt"
                    }
                  },
                  {
                    "value": ": any member of a household, who is not a full-time student, is either unemployed or long-term sick",
                    "textStyle": {
                      "fontSize": "14pt"
                    }
                  }
                ],
                "listType": "bullet"
              },
              {
                "textRuns": [
                  {
                    "value": "education",
                    "textStyle": {
                      "fontWeight": "bold",
                      "fontSize": "14pt"
                    }
                  },
                  {
                    "value": ": no person in the household has at least level 2 education (5+ GCSE passes) or is aged 16-18 and a full-time student",
                    "textStyle": {
                      "fontSize": "14pt"
                    }
                  }
                ],
                "listType": "bullet"
              },
              {
                "textRuns": [
                  {
                    "value": "health and disability",
                    "textStyle": {
                      "fontWeight": "bold",
                      "fontSize": "14pt"
                    }
                  },
                  {
                    "value": ": any person in the household has general health that is “bad” or has a long-term health problem",
                    "textStyle": {
                      "fontSize": "14pt"
                    }
                  }
                ],
                "listType": "bullet"
              },
              {
                "textRuns": [
                  {
                    "value": "housing",
                    "textStyle": {
                      "fontWeight": "bold",
                      "fontSize": "14pt"
                    }
                  },
                  {
                    "value": ": the household’s accommodation is either overcrowded or is in a shared dwelling, or has no central heating.",
                    "textStyle": {
                      "fontSize": "14pt"
                    }
                  }
                ],
                "listType": "bullet"
              }
            ]
          }
